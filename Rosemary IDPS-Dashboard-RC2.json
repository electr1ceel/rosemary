          "query": "from(bucket: \"suricata_events\")\n  |> range(start: -12h)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"suricata\")\n  |> filter(fn: (r) => r[\"event_type\"] != \"stats\")  \n  |> pivot(rowKey: [\"_time\"], columnKey: [\"_field\"], valueColumn: \"_value\")\n  |> filter(fn: (r) => r[\"action\"] == \"blocked\")\n  |> sort(columns: [\"_time\"], desc: true)\n  |> map(fn: (r) => ({\n    time: r._time,\n    action: r.action,\n//    event_type: r.event_type,\n    signature: r.signature,\n    signature_id: string(v: r.signature_id),\n//    category: r.category,\n    protocol: r.proto,\n    src_ip: r.src_ip,\n    src_port: string(v: r.src_port),\n    dest_ip: r.dest_ip,\n    dest_port: string(v: r.dest_port)\n  }))\n  |> drop(columns: [\"_start\", \"_stop\", \"_measurement\"])\n  |> limit(n: 100, offset: 0)\n  |> yield(name: \"table\")",
          "refId": "A"
        }
